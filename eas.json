{
    "cli": {
      "version": ">= 16.0.1",
      "appVersionSource": "remote"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal",
        "env": {
          "APP_VARIANT": "development"
        },
        "channel": "development"
      },
      "preview": {
        "distribution": "internal",
        "env": {
          "APP_VARIANT": "preview"
        },
        "channel": "preview"
      },
      "production": {
        "autoIncrement": true,
        "channel": "production"
      }
    },
    "submit": {
      "production": {}
    }
}